@model ITOfficeManagementSystem.Models.Feedback

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Feedback - IT_OMS</title>
    <!-- Custom fonts for this template-->
    <link href="~/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <!-- Custom styles for this template-->
    <link href="~/css/sb-admin-2.min.css" rel="stylesheet">
</head>

<body style="text-align: center;">
    <h4>Give Feedback For Improve The System</h4>
    <hr />
    <div class="container">
        <div class="text-success"><center><h3>@ViewBag.Success</h3></center></div>
        <div asp-validation-summary="All"></div>
        <form class="form-horizontal" asp-controller="Feedback" asp-action="AddFeedback" method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <fieldset class="form-group">
                <div class="row">
                    <label class="col-form-label pt-0">1. How satisfied are you with the reliability of this software? *</label>
                </div>
                <div class="row">
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="Reliability" asp-for="Reliability" value="Extremely_Satisfied">
                            <label class="form-check-label" asp-for="Reliability">Extremely Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="Reliability" asp-for="Reliability" value="Very_Satisfied">
                            <label class="form-check-label" asp-for="Reliability">Very Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="Reliability" asp-for="Reliability" value="Satisfied">
                            <label class="form-check-label" asp-for="Reliability">Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="Reliability" asp-for="Reliability" value="Not_Satisfied">
                            <label class="form-check-label" asp-for="Reliability">Not Satisfied</label>
                        </div>
                    </div>
                </div>
            </fieldset>
            <hr />

            <fieldset class="form-group">
                <div class="row">
                    <label class="col-form-label pt-1">2. How satisfied are you with the look and feel of this software? *</label>
                </div>
                <div class="row">
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="Design" asp-for="Design" value="Extremely_Satisfied">
                            <label class="form-check-label" asp-for="Design">Extremely Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="Design" asp-for="Design" value="Very_Satisfied">
                            <label class="form-check-label" asp-for="Design">Very Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="Design" asp-for="Design" value="Satisfied">
                            <label class="form-check-label" asp-for="Design">Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="Design" asp-for="Design" value="Not_Satisfied">
                            <label class="form-check-label" asp-for="Design">Not Satisfied</label>
                        </div>
                    </div>
                </div>
            </fieldset>
            <hr />

            <fieldset class="form-group">
                <div class="row">
                    <label class="col-form-label pt-2">3. How satisfied are you with this software's ease of use? *</label>
                </div>
                <div class="row">
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="EaseOfUse" asp-for="EaseOfUse" value="Extremely_Satisfied">
                            <label class="form-check-label" asp-for="EaseOfUse">Extremely Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="EaseOfUse" asp-for="EaseOfUse" value="Very_Satisfied">
                            <label class="form-check-label" asp-for="EaseOfUse">Very Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="EaseOfUse" asp-for="EaseOfUse" value="Satisfied">
                            <label class="form-check-label" asp-for="EaseOfUse">Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="EaseOfUse" asp-for="EaseOfUse" value="Not_Satisfied">
                            <label class="form-check-label" asp-for="EaseOfUse">Not Satisfied</label>
                        </div>
                    </div>
                </div>
            </fieldset>
            <hr />

            <fieldset class="form-group">
                <div class="row">
                    <label class="col-form-label pt-3">4. How satisfied are you with the security of this software? *</label>
                </div>
                <div class="row">
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="Security" asp-for="Security" value="Extremely_Satisfied">
                            <label class="form-check-label" asp-for="Security">Extremely Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="Security" asp-for="Security" value="Very_Satisfied">
                            <label class="form-check-label" asp-for="Security">Very Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="Security" asp-for="Security" value="Satisfied">
                            <label class="form-check-label" asp-for="Security">Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="Security" asp-for="Security" value="Not_Satisfied">
                            <label class="form-check-label" asp-for="Security">Not Satisfied</label>
                        </div>
                    </div>
                </div>
            </fieldset>
            <hr />

            <fieldset class="form-group">
                <div class="row">
                    <label class="col-form-label pt-4">5. How satisfied are you with the ability to integrate other software with this software? *</label>
                </div>
                <div class="row">
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="AbilityToIntegrate" asp-for="AbilityToIntegrate" value="Extremely_Satisfied">
                            <label class="form-check-label" asp-for="AbilityToIntegrate">Extremely Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="AbilityToIntegrate" asp-for="AbilityToIntegrate" value="Very_Satisfied">
                            <label class="form-check-label" asp-for="AbilityToIntegrate">Very Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="AbilityToIntegrate" asp-for="AbilityToIntegrate" value="Satisfied">
                            <label class="form-check-label" asp-for="AbilityToIntegrate">Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="AbilityToIntegrate" asp-for="AbilityToIntegrate" value="Not_Satisfied">
                            <label class="form-check-label" asp-for="AbilityToIntegrate">Not Satisfied</label>
                        </div>
                    </div>
                </div>
            </fieldset>
            <hr />

            <fieldset class="form-group">
                <div class="row">
                    <label class="col-form-label pt-5">6. How satisfied are you with the ability to collaborate with other users on this software? *</label>
                </div>
                <div class="row">
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="AbilityToCollaborate" asp-for="AbilityToCollaborate" value="Extremely_Satisfied">
                            <label class="form-check-label" asp-for="AbilityToCollaborate">Extremely Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="AbilityToCollaborate" asp-for="AbilityToCollaborate" value="Very_Satisfied">
                            <label class="form-check-label" asp-for="AbilityToCollaborate">Very Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="AbilityToCollaborate" asp-for="AbilityToCollaborate" value="Satisfied">
                            <label class="form-check-label" asp-for="AbilityToCollaborate">Satisfied</label>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="AbilityToCollaborate" asp-for="AbilityToCollaborate" value="Not_Satisfied">
                            <label class="form-check-label" asp-for="AbilityToCollaborate">Not Satisfied</label>
                        </div>
                    </div>
                </div>
            </fieldset>
            <hr />

            <div class="form-group row">
                <label asp-for="Description" class="col-form-label">7. Do you have any thoughts on how to improve this software? (Optional)</label>
                <div class="col-sm-12">
                    <textarea asp-for="Description" rows="5" class="form-control"></textarea>
                    <span asp-validation-for="Description" class="text-danger"></span>
                </div>
            </div>

            <div class="form-group row">
                <label asp-for="ClientId" class="col-sm-2 col-form-label">Client Id: </label>
                <div class="col-sm-10">
                    <select asp-items="@ViewBag.Clientid" asp-for="ClientId" class="form-control">
                        <option hidden disabled selected>Select Client Id</option>
                    </select>
                    <span asp-validation-for="ClientId" class="text-danger"></span>
                </div>
            </div>

            <div class="form-group" style="padding-left: 10%;">
                <input type="submit" value="Submit" class="btn btn-outline-success" /> |
                <a asp-controller="Feedback" asp-action="FeedbackList" class="btn btn-outline-info">Back to List</a>
            </div>
        </form>
    </div>
    <!-- Bootstrap core JavaScript-->
    <script src="~/vendor/jquery/jquery.min.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- Core plugin JavaScript-->
    <script src="~/vendor/jquery-easing/jquery.easing.min.js"></script>
    <!-- Custom scripts for all pages-->
    <script src="~/js/sb-admin-2.min.js"></script>
    @section Scripts {
        @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    }
</body>
</html>